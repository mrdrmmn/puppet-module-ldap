<%-
  config = scope.to_hash
  dn_map = Hash.new

  seen = Hash.new
  config['db_mapping'].each do |line|
    key, value = line.split( /\s*:\s*/ ).values_at( 0, 4 )
    next if ! key or key == '' or seen.has_key?( key ) or ! value or value == ''
    seen[ key ] = 1
    dn_map[ key ] = value
  end

  if config['ssl_mode'] == 'yes'
    port = config['ssl_port']
  else
    port = config['port']
  end
  
